@WEBSERVICE_TRUE@	$(ANT) -f $(OPAL_XML) undeploy -DserviceName=FIMO_${VERSION} -DappConfig=${sysconfdir}/fimo_config.xml
@WEBSERVICE_TRUE@	$(ANT) -f $(OPAL_XML) undeploy -DserviceName=MEME_${VERSION} -DappConfig=${sysconfdir}/meme_config.xml
@WEBSERVICE_TRUE@	$(ANT) -f $(OPAL_XML) undeploy -DserviceName=MAST_${VERSION} -DappConfig=${sysconfdir}/mast_config.xml
@WEBSERVICE_TRUE@	$(ANT) -f $(OPAL_XML) undeploy -DserviceName=MCAST_${VERSION} -DappConfig=${sysconfdir}/mcast_config.xml
@WEBSERVICE_TRUE@	$(ANT) -f $(OPAL_XML) undeploy -DserviceName=GLAM2_${VERSION} -DappConfig=${sysconfdir}/glam2_config.xml
