logo.js.xsl: logo.js
	sed -e 's/&/\&amp;/g' -e 's/</\&lt;/g' -e 's/>/\&gt;/g' $? > $?.tmp \
    && sed 's/tname/$?/g' xsl-wrapper.in | sed '3r $?.tmp' > $@ \
    && rm -f $?.tmp

